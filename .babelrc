{
    "presets": [
        "next/babel"
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "."
                ],
                "alias": {
                    "@root": "./",
                    "@components": "./src/components",
                    "@core": "./src/core",
                    "@custom": "./src/custom",
                    "@i18n": "./src/lib/i18n",
                    "@pages": "./src/pages",
                    "@theme": "./src/theme",
                    "@helpers": "./src/helpers",
                    "@services": "./src/services",
                    "@config" : "./swift.config.js",
                    "@lib": "./src/lib",
                    "@stores": "./src/stores",
                    "@layout" : "./src/core/theme/layout",
                    //commons components
                    "@common_button": "./src/core/commons/Button",
                    "@common_breadcrumb": "./src/core/commons/Breadcrumb",
                    "@common_confirmdialog": "./src/core/commons/ConfirmDialog",
                    "@common_dropfile": "./src/core/commons/DropFile",
                    "@common_googlemaps": "./src/core/commons/GoogleMaps",
                    "@common_gridlist": "./src/core/commons/GridList",
                    "@common_priceformat": "./src/core/commons/PriceFormat",
                    "@common_ratingstar": "./src/core/commons/RatingStar",
                    "@common_tabs": "./src/core/commons/Tabs",
                    "@common_toast": "./src/core/commons/Toast",
                    "@common_typography": "./src/core/commons/Typography",
                    "@common_checkbox": "./src/core/commons/CheckBox",
                    "@common_password": "./src/core/commons/Password",
                    "@common_radio": "./src/core/commons/Radio",
                    "@common_rangeslider": "./src/core/commons/RangeSlider",
                    "@common_select": "./src/core/commons/Select",
                    "@common_textfield": "./src/core/commons/TextField",
                    "@common_banner": "./src/core/commons/Banner",
                    "@common_carousel": "./src/core/commons/carousel",
                    "@common_image": "./src/core/commons/Image",
                    "@common_backdrop": "./src/core/commons/Backdrop",
                    "@common_pageprogress": "./src/core/commons/PageProgress",
                    "@common_header": "./src/core/theme/components/header/mobile",
                    "@common_slider": "./src/core/commons/Slider",
                    "@common_filterdialog": "./src/core/commons/FilterDialog",
                    "@common_forms": "./src/core/commons/Forms",
                    "@common_searchmodal": "./src/core/commons/SearchModal",
                    "@common_loaders": "./src/core/commons/Loaders",
                    "@common_navigation": "./src/core/commons/Navigation",
                    "@common_bottomnavigation": "./src/core/theme/components/bottom-navigation/mobile",
                    "@common_headerdesktop": "./src/core/theme/components/header/desktop",
                    "@common_spancategory": "./src/core/commons/SpanCategory",
                    "@common_span": "./src/core/commons/Span",
                    "@common_skeleton": "./src/core/commons/Skeleton",
                    "@common_footer": "./src/core/theme/components/footer/desktop"
                }
            }
        ]
    ]
}